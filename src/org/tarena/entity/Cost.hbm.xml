<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.tarena.entity">
	<class name="Cost" table="COST">
		<id name="id" type="java.lang.Integer">
			<column name="ID"></column>
		</id>

		<property name="name" type="java.lang.String">
			<column name="NAME"></column>
		</property>

		<property name="baseDuration" type="java.lang.Float">
			<column name="BASE_DURATION"></column>
		</property>

		<property name="baseCost" type="java.lang.Float">
			<column name="BASE_COST"></column>
		</property>

		<property name="unitCost" type="java.lang.Float">
			<column name="UNIT_COST"></column>
		</property>

		<property name="status" type="java.lang.String">
			<column name="STATUS"></column>
		</property>

		<property name="descr" type="java.lang.String">
			<column name="DESCR"></column>
		</property>

		<property name="startTime" type="java.sql.Date">
			<column name="STARTTIME"></column>
		</property>

		<property name="costType" type="java.lang.String">
			<column name="COST_TYPE"></column>
		</property>

	</class>


</hibernate-mapping>